{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "College Information",
    "type": "object",
    "properties": {
        "college-name": {
            "type": "string",
            "description": "Name of the College/University"
        },
        "course-level": {
            "type": "string",
            "description": "Level of the course (undergraduate, diploma, etc)"
        },
        "course-name": {
            "type": "string",
            "description": "Name of the course (B.Tech, B.Com, etc)"
        },
        "branch-name": {
            "type": "string",
            "description": "Name of the branch (CSE, CE, IT, etc)"
        },
        "city": {
            "type": "string"
        },
        "state": {
            "type": "string"
        }
    }
}